{
    "name": "jurager/tracker",
    "description": "Track Sanctum tokens in Laravel.",
    "license": "MIT",
    "keywords": ["laravel", "sanctum", "tracker", "history", "logins", "auth"],
    "authors": [
        {
            "name": "Yuri Gerassimov",
            "email": "jurager01@gmail.com",
            "homepage": "https://gerassimov.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "laravel/sanctum": "^3.0|^4.0",
        "nesbot/carbon": "^2.55.2|^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^10.5|^11.5",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "whichbrowser/parser": "^2.1",
        "jenssegers/agent": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Jurager\\Tracker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jurager\\Tracker\\Tests\\": "tests",
            "Jurager\\Tracker\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jurager\\Tracker\\TrackerServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
